<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nick.ui2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SpringActivity" />
        <activity android:name=".FoldableLayout.activities.FoldbaleActivity" />
        <activity android:name=".FoldableLayout.activities.FoldableListActivity" />
        <activity android:name=".FoldableLayout.activities.UnfoldableDetailsActivity" />
        <activity
            android:name=".activity.ActivityButton"
            android:label="@string/title_activity_button"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".activity.FloatingActivity"/>
        <activity android:name=".activity.CollapseCalendarActivity" />
        <activity android:name=".activity.ProgressActivity" />
        <activity android:name=".activity.CircleActivity"/>
    </application>

</manifest>